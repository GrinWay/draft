<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{{- app_text_logo -}}{% endblock %}</title>
    <link rel="icon" href="/favicon.ico">

    {% block head_inner %}{% endblock %}

    {% block metas %}
        <meta name="turbo-prefetch" content="false">
    {% endblock %}

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
    {#    <script src="{{ asset('js/fos_js_routes.js') }}"></script> #}

</head>
<body class="app-theme">
{% block menu %}
    <twig:menu></twig:menu>
{% endblock %}
<div class="container overflow-clip">
    <twig:dump></twig:dump>
    {% block body %}{% endblock %}
</div>
</body>
</html>
